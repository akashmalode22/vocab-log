{
    "name": "Vocab-Log",

    "version": "1.0",

    "description": "Log new words you come across webpages!",

    "manifest_version": 2,

    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },

    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "tabs"
    ]
}
